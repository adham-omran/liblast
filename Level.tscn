// TextureUniformTriplanar:5
	vec3 n_out5p0;
	float n_out5p1;
	{
		vec4 n_tex_read = triplanar_texture( BaseTextureNormal, triplanar_power_normal, triplanar_pos );
		n_out5p0 = n_tex_read.rgb;
		n_out5p1 = n_tex_read.a;
	}

// Output:0
	ALBEDO = n_out3p0;
	ROUGHNESS = n_out6p0;
	NORMALMAP = n_out5p0;

}

